{
	"version": 3,
	"routes": [
		{
			"handle": "filesystem"
		},
		{
			"src": "^/_astro/(.*)$",
			"headers": {
				"cache-control": "public, max-age=31536000, immutable"
			},
			"continue": true
		},
		{
			"src": "^/_server-islands/([^/]+?)/?$",
			"dest": "_render"
		},
		{
			"src": "^/_image/?$",
			"dest": "_render"
		},
		{
			"src": "^/account/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/auth(?:/(.*?))?/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/cover/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/delete/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/download/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/playlist/add/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/playlist/image/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/playlist/list/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/playlist/new/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/playlist/remove/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/playlist/track/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/playlist/tracks/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/tracks/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/upload/authorize/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/upload/finalize/?$",
			"dest": "_render"
		},
		{
			"src": "^/auth/reset/?$",
			"dest": "_render"
		},
		{
			"src": "^/auth/signin/?$",
			"dest": "_render"
		},
		{
			"src": "^/auth/signup/?$",
			"dest": "_render"
		},
		{
			"src": "^/home/?$",
			"dest": "_render"
		},
		{
			"src": "^/playlist/([^/]+?)/?$",
			"dest": "_render"
		},
		{
			"src": "^/storage/?$",
			"dest": "_render"
		},
		{
			"src": "^/$",
			"dest": "_render"
		}
	]
}