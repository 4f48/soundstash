{
	"$schema": "node_modules/wrangler/config-schema.json",
	"name": "soundstash",
	"main": "./dist/_worker.js/index.js",
	"compatibility_date": "2025-06-21",
	"compatibility_flags": ["nodejs_compat"],
	"assets": {
		"binding": "ASSETS",
		"directory": "./dist",
	},
	"observability": {
		"enabled": true,
	},
	"placement": {
		"mode": "smart",
	},
}
